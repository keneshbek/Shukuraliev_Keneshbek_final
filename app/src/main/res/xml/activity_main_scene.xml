<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick
            motion:targetId="@+id/imageView3"
            motion:clickAction="toggle"/>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="279dp"
            android:layout_width="355dp"
            android:id="@+id/imageView3"
            motion:layout_editor_absoluteX="28dp"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            android:layout_height="809dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="100dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.507"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="480dp"
            android:id="@+id/imageView6" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="359dp"
            android:layout_height="276dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView4"
            motion:layout_constraintBottom_toTopOf="@+id/imageView5"
            android:layout_marginBottom="12dp" />
    </ConstraintSet>
</MotionScene>